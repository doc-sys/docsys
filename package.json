		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"dotenv-defaults": "^1.1.1",
		"express": "~4.16.1",
		"file-type": "^13.1.2",
		"handlebars": "^4.7.6",
		"helmet": "^3.21.3",
		"i18n": "^0.8.5",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.24.0",
		"mongodb": "^3.5.6",
